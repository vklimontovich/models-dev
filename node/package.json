{
  "name": "models-dev-db",
  "version": "1.0.67",
  "description": "Up-to-date models.dev data as typed packages",
  "author": "Vladimir Klimontovich",
  "homepage": "https://github.com/vklimontovich/models-dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/vklimontovich/models-dev"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts",
    "update": "bun run scripts/update.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "release": "npm publish"
  },
  "keywords": [
    "llm",
    "models",
    "ai",
    "openai",
    "anthropic",
    "google"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "esbuild": "^0.24.0",
    "typescript": "^5.7.0"
  }
}
